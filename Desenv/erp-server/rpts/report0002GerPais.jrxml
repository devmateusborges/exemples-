	<queryString language="SQL">
		<![CDATA[select
gc.id as id_cidade,
gc.nome as nome_cidade,
gc.ativo as cidade_ativo,
gc.nr_cidade as  cidade_numero,
gu.id as id_estado,
gu.ativo as ativo_uf,
gu.nome as nome_estado,
gu.sigla_uf as sigla_uf,
gp.id as id_pais,
gp.nome as nome_pais,
gp.ativo as pais_ativo,
gp.sigla_pais as sigla_pais
from ger_pais gp 
       inner join ger_uf gu 
			 on gp.id = gu.ger_pais_id
			 inner join ger_cidade gc
			 on gu.id = gc.ger_uf_id 
			 where gc.id in (select fnutil_table_id( $P{pParCidId} ,gc.id)) 
			and gc.nome like  $P{pParCidNome}
			and gc.ativo like $P{pParCidAtivo}
			and gu.id in (select fnutil_table_id($P{pParUfId},gu.id))
			and gu.nome like $P{pParUfNome}
			and gu.ativo like $P{pParUfAtivo}
			and gu.sigla_uf in (select fnutil_table_id($P{pParUfSigla},gu.sigla_uf))
			and gp.id in (select fnutil_table_id($P{pParPaisId},gp.id)) 
			and gp.nome like $P{pParPaisNome}
			and gp.ativo  like $P{pParPaisAtivo}
			and gp.sigla_pais in (select fnutil_table_id($P{pParPaisSigla},gp.sigla_pais))
			 order by gp.sigla_pais, gu.sigla_uf, gc.nome]]>
	</queryString>
