	<queryString language="SQL">
		<![CDATA[select 
opAtivGrupo.sigla_atividade_grupo ||' - '||opAtivGrupo.nome as ope_grupo_atividade_id_desc,
opAtiv.sigla_atividade||' - '||opAtiv.nome as ope_atividade_id_desc,
'1' as tipo,
'1-RECURSOS' as tipo_desc,
opGrup.nome||' - '||opSubGrup.nome as vr_grupo_desc,
SUM(opOrdR.valor_total_util) as vr_tot,
(select sum(od1.qnt_prev_obj) from ope_centro2_ord_dest od1 where od1.ope_centro2_ord_id= opOrdAtiv.ope_centro2_ord_id) as qnt_prev_ob
from ope_centro2_ord_rec opOrdR
inner join ope_centro2_ord_ativ opOrdAtiv
on opOrdR.ope_centro2_ord_ativ_id = opOrdAtiv.id
inner join ope_atividade opAtiv
on opOrdAtiv.ope_atividade_id = opAtiv.id 
inner join ope_atividade_grupo opAtivGrupo
on opAtiv.ope_atividade_grupo_id = opAtivGrupo.id
inner join ope_centro2 opc2
on opOrdR.ope_centro2_id = opc2.id
inner join ope_centro_subgrupo opSubGrup
on opc2.ope_centro_subgrupo_id = opSubGrup.id
inner join ope_centro_grupo opGrup
on opSubGrup.ope_centro_grupo_id = opGrup.id
where opOrdAtiv.ope_centro2_ord_id = $P{pParOrdId}
group by ope_grupo_atividade_id_desc, ope_atividade_id_desc,vr_grupo_desc, opOrdAtiv.ope_centro2_ord_id
UNION ALL
select  
opAtivGrupo.sigla_atividade_grupo ||' - '||opAtivGrupo.nome as ope_grupo_atividade_id_desc,
opAtiv.sigla_atividade||' - '||opAtiv.nome as ope_atividade_id_desc,
'2' as tipo,
'2-ITEM/SERV' as tipo_desc,
gIServGrup.nome||' - '||gIServSub.nome as vr_grupo_desc,
SUM(opOrdIServ.valor_total_util) as vr_tot,
(select sum(od1.qnt_prev_obj) from ope_centro2_ord_dest od1 where od1.ope_centro2_ord_id= opOrdAtiv.ope_centro2_ord_id) as qnt_prev_obj
from ope_centro2_ord_itemserv opOrdIServ
inner  join ger_itemserv gIServ
on opOrdIServ.ger_itemserv_id = gIServ.id
inner join ger_itemserv_subgrupo gIServSub
on  gIServ.ger_itemserv_subgrupo_id = gIServSub.id
inner join ger_itemserv_grupo gIServGrup
on gIServSub.ger_grupo_id  = gIServGrup.id
inner join ope_centro2_ord_ativ opOrdAtiv
on opOrdIServ.ope_centro2_ord_ativ_id = opOrdAtiv.id
inner join ope_atividade opAtiv
on opOrdAtiv.ope_atividade_id = opAtiv.id 
inner join ope_atividade_grupo opAtivGrupo
on opAtiv.ope_atividade_grupo_id = opAtivGrupo.id
where opOrdAtiv.ope_centro2_ord_id =$P{pParOrdId}
group by ope_grupo_atividade_id_desc, ope_atividade_id_desc,vr_grupo_desc,opOrdAtiv.ope_centro2_ord_id

order by 2, 3,5]]>
