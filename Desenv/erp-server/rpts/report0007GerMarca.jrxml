	<queryString language="SQL">
		<![CDATA[SELECT  gm.id As IdMarca,
                gm.nome AS Marca,
		gm.ativo AS Ativo_Marca,
                gmm.ger_marca_id As IdModelo,
		gmm.nome AS Modelo,
		gmm.ativo AS Ativo_Modelo
FROM ger_marca gm 
left join ger_marca_modelo gmm
on gm.id = gmm.ger_marca_id
where gm.id in (select fnutil_table_id($P{pParMarcaId},gm.id))
and gm.nome like $P{pParMarcaNome}
and gm.ativo like $P{pParMarcaAtivo}
and gmm.id in (select fnutil_table_id($P{pParModeloId},gmm.id)) 
and gmm.nome like $P{pParModeloNome}
and gmm.ativo like $P{pParModeloAtivo}
order by gm.id, gmm.ger_marca_id]]>
	</queryString>
