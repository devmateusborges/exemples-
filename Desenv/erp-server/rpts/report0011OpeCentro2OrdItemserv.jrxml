	<parameter name="pParIdAtiv" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[select opItemServ.id as id_item_serv,
					 opItemServ.observacao_interna, 
					 opItemServ.observacao_externa,
					 opItemServ.qnt_rend,
					 opItemServ.perc_util,			 
					 opItemServ.qnt_total_util,
					 opItemServ.valor_unit_util,
					 opItemServ.valor_total_util,
					 opItemServ.ope_centro2_ord_ativ_id,
					 gItem.id as id_ger_item,
					 gItem.nome as nome_item_serv,
					 gItem.sigla_itemserv,
					 gUmed.id as id_unidade_medida,
					 gUmed.nome as nome_unidade_medida,
					 gUmed.sigla_umedida as sigla_unidade_medida,
					 --
					 gItemComp.id as id_ctb_comp,
					 gItemComp.nome as nome_comp,
					 gItemComp.sigla_comp,
					 gUmedComp.id as id_unidade_medida_comp,
					 gUmedComp.nome as nome_unidade_medida_comp,
					 gUmedComp.sigla_umedida as sigla_unidade_medida_comp
	from ope_centro2_ord_itemserv opItemServ
		inner join  ope_centro2_ord_ativ opOrdAtiv
		on opItemServ.ope_centro2_ord_ativ_id = opOrdAtiv.id
		left join ger_itemserv gItem
		on opItemServ.ger_itemserv_id = gItem.id
		left join ger_umedida gUmed
		on gItem.ger_umedida_id = gUmed.id
		-- Alteração com comp
		left join ctb_comp gItemComp
		on opItemServ.ctb_comp_id = gItemComp.id
		left join ger_umedida gUmedComp
		on gItemComp.ger_umedida_id = gUmedComp.id	
		where opItemServ.ope_centro2_ord_ativ_id =  $P{pParIdAtiv}]]>
	</queryString>
