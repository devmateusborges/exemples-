{
  "name": "resultfacil",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "rimraf ./build && tsc",
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --watch 'src/**' --transpile-only --ignore-watch node_modules -r tsconfig-paths/register  src/server.ts ",
    "lint": "eslint src",
    "orm": "npx ts-node ./node_modules/typeorm/cli.js",
    "test-orm-run": "npx ts-node ./node_modules/typeorm/cli.js migration:run -c test",
    "dev-orm-run": "npx ts-node ./node_modules/typeorm/cli.js migration:run -c development",
    "dev-orm-create": "npx ts-node ./node_modules/typeorm/cli.js migration:create -c development -n",
    "dev-orm-generate": "npx ts-node ./node_modules/typeorm/cli.js migration:generate -c development -n",
    "prod-orm-run": "npx ts-node ./node_modules/typeorm/cli.js migration:run -c production"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/express": "^4.17.13",
    "@types/mongoose": "^5.11.97",
    "@types/node": "16.6.2",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "prettier": "2.3.2",
    "rimraf": "3.0.2",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.5.1",
    "graphql-tools": "^7.0.5",
    "mongoose": "^5.13.2",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.6.0",
    "typeorm": "^0.2.37"
  }
}
